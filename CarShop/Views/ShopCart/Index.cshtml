@model ShopCartViewModel
@{
    ViewData["Title"] = "Кошик";
}
<div class="container mt-5">
    @foreach(var el in Model.shopCart.ListShopItems)
    {
        <div class="alteralter-warning mt-3">
            <b>Книга:</b> @el.Car.Brand<br/>
            <b>Ціна:</b> @el.Car.Model<br/>
        </div>
    }
    <hr/>
    <a class="btn btn-danger mb-5" asp-controller="Home" asp-action="Index">Оплатити</a>
</div>