@model IEnumerable<CarShop.Models.Order>

@{
    ViewData["Title"] = "Покупка завершена";
}
<div>
    @foreach (var item in ViewBag.Orders)
   {
          <h3>Дякуємо, @item.FullName, за замовлення!</h3>
          <a>Ваше авто зарезервовано.</a>
          <a>Ви можете його забрати за адресою...</a>
          <a>Дата замовлення @item.DateBuy</a>
   }

</div>